{
    "title": "松鼠学苑",
    "description": "松鼠学苑 Gitbook",
    "author": "李少飞",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "copy-code-button",
        "-lunr", "-search", "search-plus", "search-pro",
        "github",
        "emphasize",
        "splitter",
        "-sharing",
        "-highlight",
        "tbfed-pagefooter",
        "anchors",
        "-livereload",
        "simple-page-toc",
        "sitemap-general",
        "prism",
        "prism-themes",
        "sectionx",
        "back-to-top-button",
        "cuav-chapters",
        "heading-anchors",
        "theme-fexa",
        "disqus"
    ],

    "pluginsConfig": {
        "sytles": {
            "website": "styles/website.css"
        },
        "fontsettings": {
            "theme": "white",
            "family": "sans",
            "size": 1
        },
        "github": {
            "url": "https://github.com/SquirrelAcademy"
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-okaidia.css"
            ]
        },
        "theme-default": {
            "showLevel": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © 松鼠学苑 2019",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        },
        "sitemap-general": {
            "prefix": "http://www.51deepmind.com/gitbook"
        },
        "sectionx": {
            "tag": "b"
        },
        "theme-fexa":{
            "search-placeholder":"输入关键字",
            "logo":"assets/images/logo.png",
            "favicon": "assets/images/favicon.ico"
        },
        "disqus": {
            "shortName": "zerolee"
        }
    }
}
